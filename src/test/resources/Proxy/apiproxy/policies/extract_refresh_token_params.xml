<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables enabled="true" continueOnError="false" async="false" name="extract_refresh_token_params">
    <Source clearPayload="false">request.content</Source>
    <JSONPayload>
        <Variable name="local_granttype">
            <JSONPath>$.grant_type</JSONPath>
        </Variable>
        <Variable name="refresh_token">
            <JSONPath>$.refresh_token</JSONPath>
        </Variable>
    </JSONPayload>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</ExtractVariables>
