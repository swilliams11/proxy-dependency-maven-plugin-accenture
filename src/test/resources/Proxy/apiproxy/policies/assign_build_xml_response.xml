<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="assign_build_xml_response">
    <Set>
        <Payload contentType="text/xml">
            <response>
                <access_token>{oauthv2accesstoken.oauthv2_gen_accesstoken.access_token}</access_token>
                <access_token_expire>{oauthv2accesstoken.oauthv2_gen_accesstoken.expires_in}</access_token_expire>
                <refresh_token>{oauthv2accesstoken.oauthv2_gen_accesstoken.refresh_token}</refresh_token>

            </response>
        </Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>
